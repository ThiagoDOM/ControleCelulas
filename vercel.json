{
    "version": 2,
    "regions": ["iad1"],
    "builds": [
        { "src": "/public/index.php", "use": "vercel-php@0.6.1" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "/(.js)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.html)",
        "dest": "/public/offline.html"
      },
      {
        "src": "/(.ico)",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/(.png)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.xml)",
        "dest": "/public/browserconfig.xml"
      },

      {
        "src": "/manifest.webmanifest",
        "dest": "/public/manifest.webmanifest"
      },

      {
        "src": "/robots.txt",
        "dest": "/public/robots.txt"
      },
      {
        "src": "/safari-pinned-tab.svg",
        "dest": "/public/safari-pinned-tab.svg"
      },

      {
        "src": "/build/(.*)",
        "dest": "/public/build/$1"
      },

      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ],
    "env": {
      "APP_NAME": "Controle de CÃ©lulas",
      "APP_ENV": "production",
      "APP_URL":"https://celulas.thiagodom.dev.br",
      "ASSET_URL":"https://celulas.thiagodom.dev.br",
      "APP_DEBUG": "true",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "VIEW_COMPILED_PATH": "/tmp/views",
      "SSR_TEMP_PATH": "/tmp/ssr",
      "NODE_PATH": "node"
    }
  }
