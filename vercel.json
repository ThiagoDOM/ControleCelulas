{
    "version": 2,
    "builds": [
        { "src": "/api/index.php", "use": "vercel-php@0.5.3" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "public/$1/$2"
        },
      {
        "src": "/build/(.*)",
        "dest": "/public/build/"
      },
      {
        "src": "/resources/(.*)",
        "dest": "/public/resources/"
      },
      {
        "src": "/favicon/(.*)",
        "dest": "/public/favicon/"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_NAME": "personalBlog",
      "APP_ENV": "production",
      "APP_URL":"https://controle-celulas.vercel.app",
      "ASSET_URL":"https://controle-celulas.vercel.app",
      "APP_DEBUG": "true",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "VIEW_COMPILED_PATH": "/tmp",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "VIEW_COMPILED_PATH": "/tmp/views",
      "SSR_TEMP_PATH": "/tmp/ssr",
      "NODE_PATH": "node"
    }
  }
