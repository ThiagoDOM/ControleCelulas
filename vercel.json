{
    "version": 2,
    "regions": ["iad1"],
    "builds": [
        { "src": "/public/index.php", "use": "vercel-php@0.6.1" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "/sw.js",
        "dest": "/public/sw.js"
      },
      {
        "src": "/offline.html",
        "dest": "/public/offline.html"
      },
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/android-chrome-192x192.png",
        "dest": "/public/android-chrome-192x192.png"
      },
      {
        "src": "/android-chrome-512x512.png",
        "dest": "/public/android-chrome-512x512.png"
      },
      {
        "src": "/apple-touch-icon.png",
        "dest": "/public/apple-touch-icon.png"
      },
      {
        "src": "/browserconfig.xml",
        "dest": "/public/browserconfig.xml"
      },
      {
        "src": "/favicon-16x16.png",
        "dest": "/public/favicon-16x16.png"
      },
      {
        "src": "/favicon-32x32.png",
        "dest": "/public/favicon-32x32.png"
      },
      {
        "src": "/manifest.webmanifest",
        "dest": "/public/manifest.webmanifest"
      },
      {
        "src": "/mstile-150x150.png",
        "dest": "/public/mstile-150x150.png"
      },
      {
        "src": "/robots.txt",
        "dest": "/public/robots.txt"
      },
      {
        "src": "/safari-pinned-tab.svg",
        "dest": "/public/safari-pinned-tab.svg"
      },

      {
        "src": "/build/(.*)",
        "dest": "/public/build/"
      },
      {
        "src": "/resources/(.*)",
        "dest": "/public/resources/"
      },
      {
        "src": "/favicon/(.*)",
        "dest": "/public/favicon/"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ],
    "env": {
      "APP_NAME": "Controle de CÃ©lulas",
      "APP_ENV": "production",
      "APP_URL":"https://celulas.thiagodom.dev.br",
      "ASSET_URL":"https://celulas.thiagodom.dev.br",
      "APP_DEBUG": "true",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "VIEW_COMPILED_PATH": "/tmp/views",
      "SSR_TEMP_PATH": "/tmp/ssr",
      "NODE_PATH": "node"
    }
  }
